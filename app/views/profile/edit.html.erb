<h1>Edit profile</h1>

<div>
	<%= form_for(@profile) do |f| %>
		<%= f.label :firstname %>
    <%= f.text_field :first_name %>

    <%= f.label :lastname %>
    <%= f.email_field :last_name %>

    <%= f.label :dateofbirth %>
    <%= f.date_field :dob %>

    <%= f.label :mobilenumber %>
    <%= f.number_field :MobNum %>

    <%= f.submit :update %>
	<% end %>
</div>
